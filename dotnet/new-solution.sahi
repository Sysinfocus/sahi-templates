
PARAM "%a" "SimpleUI"
INIT "\sahi-repo\dotnet\ui-api" "\%a"
RENAME "ProjectName" "%a"
REPLACEALL "*.sln;*.bat;*.csproj;*.cs;*.razor;*.html" "ProjectName" "%a"
RUN "dotnet" "new" "gitignore"
RUN "git" "init"
RUN "\%a\migrate.bat" "Initial"
RUN "\%a\update.bat"
RUN "git" "add" "."
RUN "git" "commit" "-m" "Initial"